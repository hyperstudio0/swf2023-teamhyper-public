{
  "name": "swf2023",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "gController": "nest generate controller",
    "gService": "nest generate service",
    "build": "nest build",
    "build:api": "nest build --api",
    "build:admin": "nest build --admin",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:api:dev": "cross-env NODE_ENV=development MODULE_ENV=api nest start --watch",
    "start:admin:dev": "cross-env NODE_ENV=development MODULE_ENV=admin nest start --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "start:api:prod": "cross-env NODE_ENV=production MODULE_ENV=api node dist/main",
    "start:admin:prod": "cross-env NODE_ENV=production MODULE_ENV=admin node dist/main",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "aws-sdk": "^2.1411.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.1.4",
    "ethers": "^6.6.7",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.7",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "nestjs-i18n": "^10.2.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-typescript-api": "^12.0.4",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.16",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "web3": "^4.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "workspaces": {
    "packages": [
      "api",
      "admin"
    ]
  }
}
